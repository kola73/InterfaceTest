[
  {
    "description": "这是一个带有headers的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "json": {
        "name": "kola",
        "age": "18"
      }
    },
    "response": {
      "json": {
        "name": "kola",
        "status": "1"
      }
    }
  }
]